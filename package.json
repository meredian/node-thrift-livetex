{
  "name": "node-thrift-livetext",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/mocha --harmony --compilers js:babel/register ./test/ --recursive -R nyan",
    "thrift": "npm run thrift:compile && npm run thrift:cleanup",
    "thrift:compile": "thrift -o lib -I thrift -r -v --gen js:node thrift/EntityTagger.thrift",
    "thrift:cleanup": "node lib/cleanThrift",
    "server": "node --harmony lib/server.js",
    "client": "node --harmony lib/client.js",
    "start": "npm run server"
  },
  "author": "Anton Sidelnikov <ndmeredian@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.21",
    "bluebird": "^2.9.34",
    "node-int64": "^0.4.0",
    "thrift": "^0.9.2"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "mocha": "^2.2.5",
    "mochawait": "^2.0.1"
  }
}
